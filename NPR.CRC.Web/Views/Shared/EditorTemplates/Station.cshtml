@model System.Int64?
@using InfoConcepts.Library.Utilities
@using InfoConcepts.Library.Extensions
@using NPR.CRC.Library.DataAccess
@{
    var stationList = CRCDataAccess.GetEnabledStationList().ToSelectListItems()
        .OrderBy(li => li.Text)
        .AddBlankFirstItem()
        .SetSelected(Model);
}
@Html.DropDownListFor(model => model, stationList)

